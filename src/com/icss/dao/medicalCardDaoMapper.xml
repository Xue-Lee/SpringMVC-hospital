<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.icss.dao.MedicalCardDao">
	<select id="findMedicalCardAll" resultType="card" parameterType="card">
		select * from medicalcard where cardId = #{cardId} and pwd = #{pwd};
	</select>

</mapper>